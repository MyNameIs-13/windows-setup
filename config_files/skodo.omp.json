{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "black": "#111111",
    "pop_dark_gray": "#564E4A",
    "pop_dark_grey": "#564E4A",
    "pop_orange": "#FA9C05",
    "pop_turqouise": "#308F9C",
    "pop_white": "#FBF1C7",
    "red": "#FF0000",
    "white": "#FFFFFF"
  },
  "console_title_template": "{{ .UserName }}@{{ .HostName }} in {{ .Folder }}",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "properties": {
            "always_enabled": false,
            "cache_duration": "none"
          },
          "template": " <p:red>\uf00d</> {{ reason .Code }}",
          "foreground": "p:pop_white",
          "background": "transparent",
          "type": "status",
          "style": "plain"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "properties": {
            "always_enabled": false,
            "cache_duration": "none",
            "style": "austin",
            "threshold": 500
          },
          "template": "\ueba2 {{ .FormattedMs }} ¬∑ ",
          "foreground": "p:pop_white",
          "background": "transparent",
          "type": "executiontime",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none",
            "time_format": "02 Jan 06 15:04 MST"
          },
          "leading_diamond": "\ue0c7",
          "trailing_diamond": "\ue0c6",
          "template": "{{ .CurrentDate | date .Format }}",
          "foreground": "p:pop_white",
          "background": "transparent",
          "type": "time",
          "style": "diamond"
        }
      ],
      "newline": true
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "transparent new line",
          "foreground": "transparent",
          "background": "transparent",
          "type": "text",
          "style": "plain"
        }
      ],
      "newline": true
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "\u250c",
          "foreground": "p:pop_dark_gray",
          "background": "transparent",
          "type": "text",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none",
            "hide_root_location": false,
            "max_depth": 6,
            "style": "agnoster_short"
          },
          "trailing_diamond": "\ue0c6",
          "template": " {{ if .Writable }}{{ .Path }}{{ else }}<p:red>{{ .Path }}</>{{ end }} ",
          "foreground": "p:pop_white",
          "background": "p:pop_dark_gray",
          "type": "path",
          "style": "diamond"
        }
      ],
      "newline": true
    },
